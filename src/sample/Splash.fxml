<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<AnchorPane id="AnchorPane" fx:id="parent" onMouseClicked="#click"
            xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="sample.SplashController"
            prefHeight="480.0" prefWidth="600.0">
    <children>
        <ImageView fx:id="splash" fitHeight="479.0"
                   fitWidth="638.6" layoutX="1.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../resource/splash.png "/>
            </image>
        </ImageView>
    </children>

</AnchorPane>
